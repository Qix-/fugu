QMenu {
     background-color: #272727; /* sets background of the menu */
     color: #c8c8c8;
     border: 1px solid black;     
 }

 QMenu::item {
     /* sets background of menu item. set this to something non-transparent
         if you want menu color and menu item color to be different */
      background-color: #272727; 
      /* background-color: black; */
     
     /* padding: 2px 25px 2px 20px; */
     padding: 2px 25px 2px 20px;
    /*  border: 1px solid transparent; reserve space for selection border */
 }

 QMenu::item:selected { /* when user selects item using mouse or keyboard */
     background-color: #f27000;
 }
 
  QMenu::item:checked {
     background: #3f3f3f;
 }
 
  QMenu::item:checked:selected {
     background-color: #f27000;
 }
 
 QMenu::icon:checked { /* appearance of a 'checked' icon */
     background: transparent; /*#272727; */
     border: 0; /* 1px inset gray; */
     position: absolute;
     top: 1px;
     right: 1px;
     bottom: 1px;
     left: 1px;
 }
  
 QMenu::indicator {
     width: 13px;
     height: 13px;
     background-color: transparent; /* #272727; */
     border: 0;
     padding: 0;
     margin: 0;
 }
 
 /* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
  QMenu::indicator:non-exclusive {
 	image: none;
 	 border: 0;
     padding: 0;
     margin: 0;
 }
 
 QMenu::indicator:non-exclusive:unchecked {
 }

 QMenu::indicator:non-exclusive:unchecked:selected {
 	background-color: #f27000;
 }

 QMenu::indicator:non-exclusive:checked {
     /* image: url(&quot;../assets/icons/tick.png&quot;); */     
     background: #3f3f3f;
 }

 QMenu::indicator:non-exclusive:checked:selected {
 	background-color: #f27000;
 }

QMenu::separator {
	background: transparent; /* #3f3f3f; */
}

 /* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
 QMenu::indicator:exclusive {
 image: none;
 	 border: 0;
     padding: 0;
     margin: 0;
 }
 
 QMenu::indicator:exclusive:unchecked {
 }

